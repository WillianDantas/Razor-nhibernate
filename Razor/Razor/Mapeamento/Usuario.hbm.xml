<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Razor"
                   namespace="Razor.Models">
  <class name="Usuario">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Nome"/>
    <property name="Senha"/>
    <property name="Email"/>
  </class>
</hibernate-mapping>